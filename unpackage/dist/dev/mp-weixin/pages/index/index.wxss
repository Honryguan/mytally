/**记账按钮*/
.addButton {
	width: 120rpx;
	height: 120rpx;
	position: fixed;
	bottom: 150rpx;
	right: 50rpx;
	border-radius: 30px;
	background-color: #4cd964;
	box-shadow: #0000ff;
}
.addButton::before {
	content: '';
	position: absolute;
	left: 50%;
	top: 50%;
	width: 60rpx;
	margin-left: -30rpx;
	margin-top: -3rpx;
	border-top: 6rpx solid;
	color: white;
}
.addButton::after {
	content: '';
	position: absolute;
	left: 50%;
	top: 50%;
	height: 60rpx;
	margin-left: -3rpx;
	margin-top: -30rpx;
	border-left: 6rpx solid;
	color: white;
}
.addButton:hover {
	color: blue;
}
/**记账按钮结束*/

